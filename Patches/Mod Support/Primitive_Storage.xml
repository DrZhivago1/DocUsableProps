<Patch>


    <!--Primitive Storage-->
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Primitive Storage</li>
        </mods>
        <match Class="PatchOperationSequence">
            <success>Always</success>
            <operations>
                <li Class="PatchOperationRemove">
                    <xpath>/Defs/ThingDef[defName="VBY_WoodPile"]</xpath>
                </li>
            </operations>
        </match>
    </Operation>

</Patch>